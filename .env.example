# Deployment mode: "self-hosted" or "cloud"
CHYTR_MODE=self-hosted

# Dev mode: skip license gate entirely, runs as team tier (local dev only, never production)
CHYTR_DEV_MODE=false

# Supabase (local when self-hosted, hosted when cloud)
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# License key (self-hosted only â€” get one at https://www.chytr.ai/register)
CHYTR_LICENSE_KEY=

# chytr API key (authenticates external callers: hooks, users, integrations; also used as Cursor webhook secret)
CHYTR_API_KEY=

# Public URL of this chytr instance (required for Cursor webhook callbacks)
# Example: https://app.chytr.ai or http://localhost:3000
CHYTR_PUBLIC_URL=

# Cursor API key (for launching Cloud Agents)
CURSOR_API_KEY=

# GitHub token (for repo access)
GITHUB_TOKEN=

# Approval routing (optional)
SLACK_WEBHOOK_URL=
AGENTMAIL_API_KEY=

# chytr.ai backend (do not change)
CHYTR_API_URL=https://api.chytr.ai
